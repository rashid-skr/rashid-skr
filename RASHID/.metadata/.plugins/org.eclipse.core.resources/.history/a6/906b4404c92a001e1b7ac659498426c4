SUBROUTINE JBL.VALID.RTN
    $INSERT I_COMMON
    $INSERT I_EQUATE
    $INSERT I_AA.LOCAL.COMMON

    $USING AA.Framework
    $USING EB.SystemTables
    $USING AA.Interest
    
    ArrangementId = c_aalocArrActivityRec<AA.Framework.ArrangementActivity.ArrActArrangement>
    PROP.CLASS.TRM = 'INTEREST'
    AA.Framework.GetArrangementConditions(ArrangementId,PROP.CLASS.TRM,'PRINCIPALINT','',RETURN.IDS.TRM,RETURN.VALUES.TRM,ERR.MSG.TRM)
    RETURN.VALUES.TRM = RAISE(RETURN.VALUES.TRM)
*CONVERT SM TO FM IN RETURN.VALUES.TRM
*CONVERT VM TO FM IN RETURN.VALUES.TRM
    Y.PR.INT.RT = RETURN.VALUES.TRM<AA.Interest.Interest.IntEffectiveRate>
    EB.SystemTables.setRNew(AA.Interest.Interest.IntEffectiveRate,Y.PR.INT.RT)
END
RETURN
END

  