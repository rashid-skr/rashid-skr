SUBROUTINE ALL.PRO.CLS.RTN


    $INSERT I_COMMON
    $INSERT I_EQUATE
    $INSERT I_AA.LOCAL.COMMON

    $USING EB.SystemTables
    $USING AA.Framework
    $USING AA.Interest
    $USING EB.Foundation
    $USING EB.DataAccess
    $USING EB.Interface
    $USING AC.AccountOpening
    
    GOSUB INIT
    GOSUB LOCALTABLEUPATE
    GOSUB PROCESS
        

RETURN

PROP.CLASS.TRM = 'INTEREST'
OFS.SOURCE = 'GCS'
AA.Framework.GetArrangementConditions(ArrangementId,PROP.CLASS.TRM,'PRINCIPALINT','',RETURN.IDS.TRM,RETURN.VALUES.TRM,ERR.MSG.TRM)
RETURN.VALUES.TRM = RAISE(RETURN.VALUES.TRM)
* PROP.CLASS.TRMT= 'CHARGE'
*AA.Framework.GetArrangementConditions(ArrangementId,PROP.CLASS.TRM,'PRINCIPALINT','',RETURN.IDS.TRM,RETURN.VALUES.TRM,ERR.MSG.TRM)
*PROP.CLASS.TRMT= 'COMMITMENT'
        
* AA.Framework.GetArrangementConditions(ArrangementId,PROP.CLASS.TRM,'PRINCIPALINT','',RETURN.IDS.TRM,RETURN.VALUES.TRM,ERR.MSG.TRM)
PROP.CLASS.TRMT= 'ACCOUNT'
*AA.Framework.GetArrangementConditions(ArrangementId,PROP.CLASS.TRM,'PRINCIPALINT','',RETURN.IDS.TRM,RETURN.VALUES.TRM,ERR.MSG.TRM)
AA.Framework.GetArrangementConditions(arrId,"",'ACCOUNT','',RETURN.IDS.TRM,RETURN.VALUES.TRM,ERR.MSG.TRM)