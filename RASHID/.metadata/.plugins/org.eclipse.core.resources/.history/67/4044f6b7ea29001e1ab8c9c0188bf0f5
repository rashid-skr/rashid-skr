SUBROUTINE JBL.TRAIN.REC.RTN
    $INSERT I_COMMON
    $INSERT I_EQUATE
    $INSERT I_AA.LOCAL.COMMON

    $USING AA.Framwork
    $USING ST.Customer
    $USING EB.DataAccess
    $USING EB.SystemTables
    $USING AA.Account
    FN.CUSTOMER =F.CUSTOMER
    F.CUSTOMER =''
    EB.DataAccess.Opf(FN.CUSTOMER,F.CUSTOMER)
    Y.CUS.ID =c_aalocArrActivityRec<AA.Framwork.ArrangementActivity.ArrActcustomer>
    EB.DataAccess.FRead(FN.CUSTOMER,Y.CUS.ID,R.CUS, F.CUSTOMER ,FFileid, ERR.CUS)
    Y.CUS.SHT.NM=R.CUS<ST.Customer.Customer.EbCusShortName>
    IF EB.SystemTables.getRNew(AA.Account.Account.AcShortTitle) EQ THEN
        EB.SystemTables.setRNewLast(AA.Account.Account.AcShortTitl,Y.CUS.SHT.NM)
    
  


    END
